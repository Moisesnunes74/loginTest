#include <iostream>

using namespace std;

int main()
{
    int user, password;


    cout << "Please, type your username" << endl;
    cin >> username;

    if (username == 1234) {
        cout << "Please, type your password" << endl;
        cin >> password;

        if (password == 9999) {
            cout << "Acess Allowed" << endl;
        } else {
            cout << "Incorrect Password" << endl;
        }
    } else {
        cout << "Invalid User" << endl;
    }

    return 0;
}
